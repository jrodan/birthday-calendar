<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN http://www.apple.com/DTDs/PropertyList-1.0.dtd >
<plist version="1.0">
  <dict>
    <key>Label</key>
        <string>com.jan.rodan.birthday.notification</string>
    <key>ProgramArguments</key>
        <array>
            <string>python3</string>
            <string>/PATH/TO/PROJECT/notification.py</string>
        </array>
    <key>StartInterval</key>
        <integer>3600</integer>
    <key>StandardOutPath</key>
        <string>/PATH/TO/PROJECT/log/birthday-calendar.log</string>
    <key>StandardErrorPath</key>
        <string>/PATH/TO/PROJECT/log/birthday-calendar.error.log</string>
  </dict>
</plist>